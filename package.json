{
  "name": "glovo-admin-fe-localization-tools",
  "displayName": "Glovo Admin FE. Localization tools",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:glovo-admin-fe-localization-tools.replace-with-token",
    "onCommand:glovo-admin-fe-localization-tools.generate-results"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "glovo-admin-fe-localization-tools.replace-with-token",
        "title": "Glovo localization: Set localization token"
      },
      {
        "command": "glovo-admin-fe-localization-tools.generate-results",
        "title": "Glovo localization: Generate results"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  }
}
